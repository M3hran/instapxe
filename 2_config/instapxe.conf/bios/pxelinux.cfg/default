        	APPEND initrd=boot/sum_gen7/initrd.img media=net root=/dev/ram0 splash quiet hp_fibre showopts TYPE=AUTOMATIC AUTOPOWEROFFONSUCCESS=no AUTOREBOOTONSUCCESS=yes iso1=nfs://192.168.1.3/sum_gen7 iso1opts=nolock iso1mnt=/mnt/bootdevice


