        #MENU INCLUDE boot/utilities/utilities.menu

	LABEL SUMGEN8
        	MENU LABEL ^2) HP Smart Update Manager (SUM)-- Gen 8
	        KERNEL boot/sum_gen8/vmlinuz
	        APPEND initrd=boot/sum_gen8/initrd.img media=net root=/dev/ram0 splash quiet hp_fibre showopts TYPE=AUTOMATIC AUTOPOWEROFFONSUCCESS=no AUTOREBOOTONSUCCESS=yes iso1=nfs://192.168.1.3/sum_gen8 iso1opts=nolock iso1mnt=/mnt/bootdevice

	LABEL SUMGEN7
        	MENU LABEL ^3) HP Smart Update Manager (SUM)-- Gen 7
