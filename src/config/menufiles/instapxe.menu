LABEL WDS
    MENU LABEL ^4) Windows Deployment Services (WDS)
    COM32 pxechn.c32
    append $WDS_HOST::Boot\x64\wdsnbp.com -W

MENU BEGIN
MENU LABEL ^5) Linux Distros
	LABEL Back
		MENU LABEL Back..
                TEXT HELP
                Return to previous menu.
                ENDTEXT
                MENU EXIT
                MENU SEPARATOR
		
	MENU BEGIN Ubuntu
	MENU TITLE Ubuntu 
        	LABEL Back
        	MENU LABEL Back..
        	TEXT HELP
        	Return to previous menu.
        	ENDTEXT
        	MENU EXIT
        	MENU SEPARATOR
        	MENU INCLUDE boot/ubuntu/ubuntu.menu
	MENU END

        MENU BEGIN CentOS
        MENU TITLE CentOS
                LABEL Back
                MENU LABEL Back..
                TEXT HELP
                Return to previous menu.
                ENDTEXT
                MENU EXIT
                MENU SEPARATOR
                MENU INCLUDE boot/centos/centos.menu
        MENU END
MENU END

LABEL VMware
        MENU LABEL ^6) VMware ESXi
        COM32 pxechn.c32
        append boot/vmware/pxelinux.0

MENU BEGIN Tools and Utilities
MENU LABEL ^7) Tools and Utilities
        LABEL Back
        MENU LABEL Back..
        TEXT HELP
        Return to previous menu.
        ENDTEXT
        MENU EXIT
        MENU SEPARATOR
        MENU INCLUDE boot/utilities/utilities.menu
MENU END

LABEL BootLocal
MENU LABEL ^8) BootLocal
        localboot 0
        TEXT HELP
        Boot to local hard disk.
        ENDTEXT
